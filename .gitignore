# Ignore bundler config.
/src/.bundle

# Ignore all logfiles and tempfiles.
/src/log/*
/src/tmp/*
!/src/log/.keep
!/src/tmp/.keep

# Ignore uploaded files in development
/src/storage/*

/src/node_modules
/src/yarn-error.log

/src/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/src/config/master.key
/src/public/packs
/src/public/packs-test
/src/node_modules
/src/yarn-debug.log*
/src/yarn-error.log*
/src/.yarn-integrity

# Ignore IDE Rubymine project files
.idea/*
/src/.idea
/src/src.iml
.generators

# Docker
/src/database/data/*
/src/database/import/*.sql
docker-key
docker-key.pub
**/secrets.yml
